{
  "name": "web-template-node-service",
  "version": "0.0.1-beta",
  "description": "Nest-based Node TypeScript service boilerplate",
  "author": "Fractal",
  "private": true,
  "license": "Fractal",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "format": "prettier --write .",
    "lint:fix": "eslint --fix --format stylish \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "eslint --format stylish \"{src,apps,libs,test}/**/*.ts\"",
    "test:unit": "NODE_DEBUG=jestlogger jest \"__tests__/.*\\.test\\.ts$\"",
    "test:coverage": "npm run test:unit -- --coverage",
    "test:snyk": "snyk test --severity-threshold=critical --fail-on=all",
    "test:watch": "npm run test -- --watch",
    "test:debug": "npm run test -- --runInBand",
    "test:e2e": "jest --config ./test/e2e/jest-e2e.json --runInBand",
    "docs": "nest start --config ./docsConfig.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.2.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.0",
    "@nestjs/microservices": "^10.2.0",
    "@nestjs/mongoose": "^10.0.1",
    "@nestjs/platform-express": "^10.2.0",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/swagger": "^7.1.8",
    "bunyan": "^1.8.15",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "dd-trace": "^4.12.0",
    "ioredis": "^5.3.2",
    "lodash": "^4.17.21",
    "mongoose": "^7.4.3",
    "nestjs-asyncapi": "^1.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.1.12",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "20.5.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "29.6.3",
    "lint-staged": "^14.0.1",
    "mongo-seeding-cli": "^3.7.2",
    "mongodb-memory-server": "^8.15.1",
    "mqtt": "^5.0.3",
    "prettier": "^3.0.2",
    "snyk": "^1.1204.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "peerDependencies": {
    "webpack": "^5.75.0"
  }
}
